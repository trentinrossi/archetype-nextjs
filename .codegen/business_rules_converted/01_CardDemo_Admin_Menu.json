{
  "application_metadata": {
    "name": "CardDemo Admin Menu",
    "description": "Administrative menu system for the CardDemo application that provides centralized access to various administrative functions for authenticated admin users",
    "domain": "Financial Services - Credit Card Administration"
  },
  "backend": {
    "business_entities": [
      {
        "name": "AdminUser",
        "description": "Administrative user with access to admin menu functions",
        "attributes": [
          {
            "name": "user_id",
            "type": "string",
            "length": 8,
            "required": true,
            "description": "Unique identifier for the admin user"
          },
          {
            "name": "authentication_status",
            "type": "boolean",
            "length": 1,
            "required": true,
            "description": "Indicates if user has been authenticated as admin"
          }
        ],
        "relationships": [
          {
            "related_entity": "AdminMenuOption",
            "relationship_type": "has_access_to",
            "cardinality": "one-to-many"
          }
        ]
      },
      {
        "name": "AdminMenuOption",
        "description": "Individual administrative function available in the menu",
        "attributes": [
          {
            "name": "option_number",
            "type": "numeric",
            "length": 2,
            "required": true,
            "description": "Sequential number identifying the menu option"
          },
          {
            "name": "option_name",
            "type": "string",
            "length": 35,
            "required": true,
            "description": "Descriptive name of the administrative function"
          },
          {
            "name": "program_name",
            "type": "string",
            "length": 8,
            "required": true,
            "description": "Name of the program to execute for this option"
          },
          {
            "name": "is_active",
            "type": "boolean",
            "length": 1,
            "required": true,
            "description": "Indicates if option is active or coming soon (dummy)"
          }
        ],
        "relationships": [
          {
            "related_entity": "AdminProgram",
            "relationship_type": "executes",
            "cardinality": "one-to-one"
          }
        ]
      },
      {
        "name": "UserSession",
        "description": "Current user session information",
        "attributes": [
          {
            "name": "transaction_id",
            "type": "string",
            "length": 4,
            "required": true,
            "description": "Current transaction identifier (CA00)"
          },
          {
            "name": "program_name",
            "type": "string",
            "length": 8,
            "required": true,
            "description": "Current program name (COADM01C)"
          },
          {
            "name": "from_program",
            "type": "string",
            "length": 8,
            "required": false,
            "description": "Name of the program that transferred control"
          },
          {
            "name": "from_transaction",
            "type": "string",
            "length": 4,
            "required": false,
            "description": "Transaction ID of the calling program"
          },
          {
            "name": "program_context",
            "type": "numeric",
            "length": 9,
            "required": false,
            "description": "Context information for program execution"
          },
          {
            "name": "reenter_flag",
            "type": "boolean",
            "length": 1,
            "required": true,
            "description": "Indicates if program is being reentered after user input"
          }
        ],
        "relationships": []
      }
    ],
    "business_rules": [
      {
        "id": "BR001",
        "name": "Admin Authentication Requirement",
        "description": "User must be authenticated as admin before accessing the admin menu",
        "category": "Security",
        "logic": "Program assumes user has been authenticated as admin user before reaching this menu. If no communication area is passed, user is returned to sign-on screen.",
        "conditions": [
          "Communication area length (EIBCALEN) equals zero"
        ],
        "actions": [
          "Set from program to COSGN00C",
          "Return to sign-on screen"
        ]
      },
      {
        "id": "BR002",
        "name": "Menu Option Validation",
        "description": "User input must be a valid menu option number",
        "category": "Validation",
        "logic": "Validate that user entered option is numeric, within valid range (1 to option count), and not zero",
        "conditions": [
          "Option is not numeric",
          "Option is greater than available option count",
          "Option equals zero"
        ],
        "actions": [
          "Set error flag to Y",
          "Display error message: 'Please enter a valid option number...'",
          "Redisplay menu screen"
        ]
      },
      {
        "id": "BR003",
        "name": "Menu Option Navigation",
        "description": "Valid menu option selection transfers control to corresponding program",
        "category": "Navigation",
        "logic": "When valid option is selected and program is not dummy, transfer control to the specified program with session context",
        "conditions": [
          "Option is valid",
          "Program name does not start with DUMMY"
        ],
        "actions": [
          "Set from transaction ID to current transaction",
          "Set from program to current program name",
          "Set program context to zeros",
          "Transfer control to option program"
        ]
      },
      {
        "id": "BR004",
        "name": "Coming Soon Option Handling",
        "description": "Options marked as dummy display coming soon message",
        "category": "Display",
        "logic": "If selected option program name starts with DUMMY, display coming soon message instead of transferring control",
        "conditions": [
          "Option is valid",
          "Program name starts with DUMMY"
        ],
        "actions": [
          "Display message: 'This option is coming soon ...'",
          "Set message color to green",
          "Redisplay menu screen"
        ]
      },
      {
        "id": "BR005",
        "name": "PF3 Key Navigation",
        "description": "PF3 key returns user to sign-on screen",
        "category": "Navigation",
        "logic": "When PF3 key is pressed, return control to sign-on program",
        "conditions": [
          "PF3 key is pressed"
        ],
        "actions": [
          "Set to program to COSGN00C",
          "Return to sign-on screen"
        ]
      },
      {
        "id": "BR006",
        "name": "Invalid Key Handling",
        "description": "Invalid attention keys display error message",
        "category": "Error Handling",
        "logic": "Any key other than Enter or PF3 is considered invalid and triggers error message",
        "conditions": [
          "Key pressed is not Enter",
          "Key pressed is not PF3"
        ],
        "actions": [
          "Set error flag to Y",
          "Display invalid key error message",
          "Redisplay menu screen"
        ]
      },
      {
        "id": "BR007",
        "name": "Initial Screen Display",
        "description": "On first entry, display menu screen without processing input",
        "category": "Display",
        "logic": "When program is entered for first time (not reentered), initialize screen and display menu",
        "conditions": [
          "Program reenter flag is false"
        ],
        "actions": [
          "Set program reenter flag to true",
          "Clear screen output area",
          "Send menu screen to user"
        ]
      },
      {
        "id": "BR008",
        "name": "Menu Option Count Limit",
        "description": "System supports up to 10 menu options",
        "category": "Configuration",
        "logic": "Menu can display maximum of 10 administrative options (OPTN001O to OPTN010O)",
        "conditions": [
          "Menu options are being built"
        ],
        "actions": [
          "Display options 1 through 10",
          "Ignore any options beyond 10"
        ]
      },
      {
        "id": "BR009",
        "name": "Session Context Preservation",
        "description": "Session context is maintained across program transfers",
        "category": "Session Management",
        "logic": "When transferring to another program, preserve transaction ID, program name, and context information",
        "conditions": [
          "Valid option selected",
          "Program transfer is initiated"
        ],
        "actions": [
          "Store current transaction ID as from transaction",
          "Store current program name as from program",
          "Reset program context to zeros",
          "Pass communication area to target program"
        ]
      },
      {
        "id": "BR010",
        "name": "Option Input Normalization",
        "description": "User input is normalized by removing trailing spaces and converting to numeric",
        "category": "Data Processing",
        "logic": "Extract user option by removing trailing spaces, replacing remaining spaces with zeros, and converting to numeric",
        "conditions": [
          "User has entered option value",
          "Enter key is pressed"
        ],
        "actions": [
          "Remove trailing spaces from input",
          "Replace internal spaces with zero",
          "Convert to numeric value",
          "Validate numeric conversion"
        ]
      }
    ],
    "validations": [
      {
        "field": "option_input",
        "rule": "Must be numeric",
        "error_message": "Please enter a valid option number...",
        "severity": "error"
      },
      {
        "field": "option_input",
        "rule": "Must be greater than zero",
        "error_message": "Please enter a valid option number...",
        "severity": "error"
      },
      {
        "field": "option_input",
        "rule": "Must not exceed available option count",
        "error_message": "Please enter a valid option number...",
        "severity": "error"
      },
      {
        "field": "communication_area",
        "rule": "Must be present for authenticated access",
        "error_message": "Session expired - returning to sign-on",
        "severity": "error"
      },
      {
        "field": "attention_key",
        "rule": "Must be Enter or PF3",
        "error_message": "Invalid key pressed",
        "severity": "error"
      }
    ],
    "calculations": [
      {
        "name": "Current Date Formatting",
        "description": "Format current date as MM/DD/YY for display",
        "formula": "Extract month, day, and last 2 digits of year from system date and format as MM/DD/YY",
        "input_fields": [
          "system_current_date"
        ],
        "output_field": "formatted_current_date"
      },
      {
        "name": "Current Time Formatting",
        "description": "Format current time as HH:MM:SS for display",
        "formula": "Extract hours, minutes, and seconds from system time and format as HH:MM:SS",
        "input_fields": [
          "system_current_time"
        ],
        "output_field": "formatted_current_time"
      },
      {
        "name": "Menu Option Text Construction",
        "description": "Build menu option display text from option number and name",
        "formula": "Concatenate option number, period, space, and option name",
        "input_fields": [
          "option_number",
          "option_name"
        ],
        "output_field": "menu_option_text"
      }
    ],
    "workflows": [
      {
        "name": "Admin Menu Main Flow",
        "description": "Primary workflow for admin menu display and navigation",
        "steps": [
          {
            "step_number": 1,
            "action": "Initialize program variables and clear error flags",
            "conditions": [
              "Program starts"
            ],
            "next_step": "2"
          },
          {
            "step_number": 2,
            "action": "Check for communication area presence",
            "conditions": [
              "Communication area length is zero"
            ],
            "next_step": "3_return_to_signon"
          },
          {
            "step_number": 3,
            "action": "Load communication area data",
            "conditions": [
              "Communication area exists"
            ],
            "next_step": "4"
          },
          {
            "step_number": 4,
            "action": "Check if program is being reentered",
            "conditions": [
              "Program reenter flag is false"
            ],
            "next_step": "5_initial_display"
          },
          {
            "step_number": 5,
            "action": "Display initial menu screen",
            "conditions": [
              "First time entry"
            ],
            "next_step": "end"
          },
          {
            "step_number": 6,
            "action": "Receive user input from screen",
            "conditions": [
              "Program is reentered"
            ],
            "next_step": "7"
          },
          {
            "step_number": 7,
            "action": "Evaluate attention key pressed",
            "conditions": [
              "User has pressed a key"
            ],
            "next_step": "8"
          },
          {
            "step_number": 8,
            "action": "Process Enter key - validate and navigate",
            "conditions": [
              "Enter key pressed"
            ],
            "next_step": "9"
          },
          {
            "step_number": 9,
            "action": "Process PF3 key - return to sign-on",
            "conditions": [
              "PF3 key pressed"
            ],
            "next_step": "3_return_to_signon"
          },
          {
            "step_number": 10,
            "action": "Display invalid key error",
            "conditions": [
              "Other key pressed"
            ],
            "next_step": "5_initial_display"
          }
        ]
      },
      {
        "name": "Option Selection Processing",
        "description": "Workflow for processing user menu option selection",
        "steps": [
          {
            "step_number": 1,
            "action": "Extract and normalize option input",
            "conditions": [
              "Enter key pressed"
            ],
            "next_step": "2"
          },
          {
            "step_number": 2,
            "action": "Validate option is numeric and in range",
            "conditions": [
              "Option extracted"
            ],
            "next_step": "3"
          },
          {
            "step_number": 3,
            "action": "Display validation error if invalid",
            "conditions": [
              "Option is invalid"
            ],
            "next_step": "end_with_error"
          },
          {
            "step_number": 4,
            "action": "Check if option program is dummy",
            "conditions": [
              "Option is valid"
            ],
            "next_step": "5"
          },
          {
            "step_number": 5,
            "action": "Display coming soon message",
            "conditions": [
              "Program name starts with DUMMY"
            ],
            "next_step": "end_with_message"
          },
          {
            "step_number": 6,
            "action": "Set session context and transfer to option program",
            "conditions": [
              "Program is active"
            ],
            "next_step": "end_with_transfer"
          }
        ]
      },
      {
        "name": "Screen Display Preparation",
        "description": "Workflow for preparing and sending menu screen to user",
        "steps": [
          {
            "step_number": 1,
            "action": "Populate header information with current date and time",
            "conditions": [
              "Screen needs to be displayed"
            ],
            "next_step": "2"
          },
          {
            "step_number": 2,
            "action": "Build menu options from configuration",
            "conditions": [
              "Header populated"
            ],
            "next_step": "3"
          },
          {
            "step_number": 3,
            "action": "Set error or informational message if present",
            "conditions": [
              "Menu options built"
            ],
            "next_step": "4"
          },
          {
            "step_number": 4,
            "action": "Send screen to user terminal",
            "conditions": [
              "All screen data prepared"
            ],
            "next_step": "end"
          }
        ]
      }
    ],
    "integrations": [
      {
        "name": "Sign-On Program Integration",
        "type": "Program Transfer",
        "purpose": "Return to sign-on screen when session is invalid or user exits",
        "data_exchanged": [
          "from_program_name",
          "to_program_name"
        ],
        "trigger_conditions": [
          "No communication area present",
          "PF3 key pressed",
          "Session timeout"
        ]
      },
      {
        "name": "Admin Function Programs Integration",
        "type": "Program Transfer",
        "purpose": "Transfer control to specific administrative function programs",
        "data_exchanged": [
          "from_transaction_id",
          "from_program_name",
          "program_context",
          "communication_area"
        ],
        "trigger_conditions": [
          "Valid menu option selected",
          "Option program is active (not dummy)"
        ]
      },
      {
        "name": "User Security File Access",
        "type": "Data Access",
        "purpose": "Validate user authentication and authorization",
        "data_exchanged": [
          "user_id",
          "authentication_status"
        ],
        "trigger_conditions": [
          "User attempts to access admin menu"
        ]
      }
    ],
    "security": {
      "roles": [
        "Admin User",
        "System Administrator"
      ],
      "permissions": [
        {
          "role": "Admin User",
          "resource": "Admin Menu",
          "actions": [
            "view_menu",
            "select_option",
            "navigate_to_admin_functions"
          ]
        },
        {
          "role": "System Administrator",
          "resource": "Admin Menu Configuration",
          "actions": [
            "view_menu",
            "select_option",
            "navigate_to_admin_functions",
            "configure_menu_options"
          ]
        }
      ],
      "authentication_requirements": [
        "User must be authenticated before accessing admin menu",
        "Valid communication area must be present",
        "User must have admin role privileges",
        "Session must be active and valid"
      ]
    },
    "error_handling": [
      {
        "error_code": "ERR001",
        "condition": "No communication area present (EIBCALEN = 0)",
        "business_impact": "User cannot access admin menu without valid session",
        "suggested_action": "Return to sign-on screen for authentication"
      },
      {
        "error_code": "ERR002",
        "condition": "Invalid option number entered",
        "business_impact": "User cannot navigate to desired function",
        "suggested_action": "Display error message and prompt for valid option"
      },
      {
        "error_code": "ERR003",
        "condition": "Invalid attention key pressed",
        "business_impact": "User action not recognized",
        "suggested_action": "Display invalid key message and redisplay menu"
      },
      {
        "error_code": "ERR004",
        "condition": "Option program is dummy (coming soon)",
        "business_impact": "Selected function not yet available",
        "suggested_action": "Display coming soon message and allow reselection"
      },
      {
        "error_code": "ERR005",
        "condition": "Non-numeric option entered",
        "business_impact": "Invalid input format",
        "suggested_action": "Display validation error and prompt for numeric input"
      },
      {
        "error_code": "ERR006",
        "condition": "Option number exceeds available options",
        "business_impact": "User attempting to access non-existent function",
        "suggested_action": "Display range validation error"
      },
      {
        "error_code": "ERR007",
        "condition": "Option number is zero",
        "business_impact": "Invalid selection value",
        "suggested_action": "Display validation error for zero value"
      }
    ]
  },
