        "name": "Sign-on Program",
        "type": "Program Transfer",
        "purpose": "Handles user authentication and initial sign-on",
        "data_exchanged": [
          "User session data",
          "Authentication status"
        ],
        "trigger_conditions": [
          "User not authenticated",
          "PF3 pressed to exit"
        ]
      },
      {
        "name": "Menu Option Programs",
        "type": "Program Transfer",
        "purpose": "Executes selected menu functions",
        "data_exchanged": [
          "Session context",
          "User information",
          "Transaction ID",
          "Program context"
        ],
        "trigger_conditions": [
          "Valid menu option selected",
          "User has access",
          "Option is implemented"
        ]
      }
