{
  "application_metadata": {
    "name": "CardDemo Main Menu",
    "description": "Main menu interface for regular users in the CardDemo application, providing navigation to various functions with access control and session management",
    "domain": "Financial Services - Card Management"
  },
  "backend": {
    "business_entities": [
      {
        "name": "UserSession",
        "description": "Represents the current user session and navigation context",
        "attributes": [
          {
            "name": "from_program",
            "type": "string",
            "length": 8,
            "required": true,
            "description": "Name of the calling program"
          },
          {
            "name": "to_program",
            "type": "string",
            "length": 8,
            "required": true,
            "description": "Name of the program to transfer to"
          },
          {
            "name": "program_reenter_flag",
            "type": "boolean",
            "length": 1,
            "required": true,
            "description": "Indicates if the program is being reentered"
          },
          {
            "name": "user_type",
            "type": "string",
            "length": 1,
            "required": true,
            "description": "User type indicator (regular user or admin)"
          },
          {
            "name": "transaction_id",
            "type": "string",
            "length": 4,
            "required": true,
            "description": "Current transaction identifier"
          },
          {
            "name": "program_context",
            "type": "numeric",
            "length": 4,
            "required": true,
            "description": "Context information for program execution"
          }
        ],
        "relationships": [
          {
            "related_entity": "User",
            "relationship_type": "belongs_to",
            "cardinality": "1:1"
          },
          {
            "related_entity": "MenuOption",
            "relationship_type": "has_many",
            "cardinality": "1:N"
          }
        ]
      },
      {
        "name": "MenuOption",
        "description": "Represents a menu option available to users",
        "attributes": [
          {
            "name": "option_number",
            "type": "numeric",
            "length": 2,
            "required": true,
            "description": "Sequential number of the menu option"
          },
          {
            "name": "option_name",
            "type": "string",
            "length": 40,
            "required": true,
            "description": "Display name of the menu option"
          },
          {
            "name": "program_name",
            "type": "string",
            "length": 8,
            "required": true,
            "description": "Program to execute when option is selected"
          },
          {
            "name": "required_user_type",
            "type": "string",
            "length": 1,
            "required": true,
            "description": "User type required to access this option (A for admin, U for user)"
          }
        ],
        "relationships": [
          {
            "related_entity": "UserSession",
            "relationship_type": "belongs_to",
            "cardinality": "N:1"
          }
        ]
      },
      {
        "name": "User",
        "description": "Represents a user of the CardDemo application",
        "attributes": [
          {
            "name": "user_type",
            "type": "string",
            "length": 1,
            "required": true,
            "description": "Type of user (regular or admin)"
          },
          {
            "name": "authenticated",
            "type": "boolean",
            "length": 1,
            "required": true,
            "description": "Whether the user is authenticated"
          }
        ],
        "relationships": [
          {
            "related_entity": "UserSession",
            "relationship_type": "has_one",
            "cardinality": "1:1"
          }
        ]
      }
    ],
    "business_rules": [
      {
        "id": "BR001",
        "name": "User Authentication Check",
        "description": "Verifies that the user is authenticated before allowing access to the main menu",
        "category": "Security",
        "logic": "If COMMAREA length is 0, the user is not authenticated and must be redirected to the sign-on screen",
        "conditions": [
          "COMMAREA length equals 0"
        ],
        "actions": [
          "Set from_program to COSGN00C",
          "Return to sign-on screen"
        ]
      },
      {
        "id": "BR002",
        "name": "Menu Option Validation",
        "description": "Validates that the user's menu selection is a valid numeric option within the available range",
        "category": "Validation",
        "logic": "The selected option must be numeric, greater than 0, and not exceed the total number of menu options",
        "conditions": [
          "Option is numeric",
          "Option is greater than 0",
          "Option is less than or equal to menu option count"
        ],
        "actions": [
          "If invalid, display error message: 'Please enter a valid option number...'",
          "If valid, proceed to access control check"
        ]
      },
      {
        "id": "BR003",
        "name": "Access Control by User Type",
        "description": "Enforces access restrictions based on user type, preventing regular users from accessing admin-only options",
        "category": "Security",
        "logic": "Regular users cannot access menu options marked as admin-only",
        "conditions": [
          "User type is regular user",
          "Selected menu option requires admin access"
        ],
        "actions": [
          "Display error message: 'No access - Admin Only option...'",
          "Prevent navigation to the selected option"
        ]
      },
      {
        "id": "BR004",
        "name": "Program Reentry Detection",
        "description": "Determines whether the program is being entered for the first time or reentered from another screen",
        "category": "Navigation",
        "logic": "On first entry, initialize the screen and set reentry flag; on subsequent entries, process user input",
        "conditions": [
          "Program reenter flag is false"
        ],
        "actions": [
          "Set program reenter flag to true",
          "Initialize screen to low values",
          "Send menu screen"
        ]
      },
      {
        "id": "BR005",
        "name": "Coming Soon Feature Handling",
        "description": "Handles menu options that are not yet implemented",
        "category": "Navigation",
        "logic": "If a menu option's program name starts with 'DUMMY', display a 'coming soon' message instead of navigating",
        "conditions": [
          "First 5 characters of program name equal 'DUMMY'"
        ],
        "actions": [
          "Display message: 'This option [option name] is coming soon ...'",
          "Remain on current screen"
        ]
      },
      {
        "id": "BR006",
        "name": "Session Continuity",
        "description": "Maintains session information across program transfers",
        "category": "Session Management",
        "logic": "When transferring to another program, preserve session context including transaction ID, program name, and user information",
        "conditions": [
          "Valid menu option selected",
          "User has access to the option",
          "Program is not a dummy placeholder"
        ],
        "actions": [
          "Set from_transaction_id to current transaction",
          "Set from_program to current program name",
          "Reset program context to zeros",
          "Transfer control to selected program with session data"
        ]
      },
      {
        "id": "BR007",
        "name": "Exit to Sign-on",
        "description": "Allows users to exit the menu and return to the sign-on screen",
        "category": "Navigation",
        "logic": "When PF3 is pressed, transfer control back to the sign-on program",
        "conditions": [
          "PF3 key is pressed"
        ],
        "actions": [
          "Set to_program to COSGN00C",
          "Return to sign-on screen"
        ]
      },
      {
        "id": "BR008",
        "name": "Invalid Key Handling",
        "description": "Handles invalid function key presses",
        "category": "Error Handling",
        "logic": "If a key other than Enter or PF3 is pressed, display an error message",
        "conditions": [
          "Key pressed is not Enter",
          "Key pressed is not PF3"
        ],
        "actions": [
          "Set error flag to true",
          "Display invalid key error message",
          "Redisplay menu screen"
        ]
      },
      {
        "id": "BR009",
        "name": "Dynamic Menu Building",
        "description": "Constructs menu options dynamically based on available functions and user permissions",
        "category": "Display",
        "logic": "Menu options are built from configuration data, showing up to 12 options with their numbers and descriptions",
        "conditions": [
          "Menu option count is defined",
          "Menu option data is available"
        ],
        "actions": [
          "Iterate through available menu options",
          "Format each option as 'number. description'",
          "Display formatted options on screen"
        ]
      },
      {
        "id": "BR010",
        "name": "Input Normalization",
        "description": "Normalizes user input by trimming spaces and converting to numeric format",
        "category": "Validation",
        "logic": "User input is trimmed of trailing spaces, internal spaces are replaced with zeros, and the result is converted to numeric",
        "conditions": [
          "User has entered an option"
        ],
        "actions": [
          "Trim trailing spaces from input",
          "Replace internal spaces with '0'",
          "Convert to numeric value",
          "Validate numeric result"
        ]
      }
    ],
    "validations": [
      {
        "field": "option",
        "rule": "Must be numeric",
        "error_message": "Please enter a valid option number...",
        "severity": "error"
      },
      {
        "field": "option",
        "rule": "Must be greater than 0",
        "error_message": "Please enter a valid option number...",
        "severity": "error"
      },
      {
        "field": "option",
        "rule": "Must not exceed menu option count",
        "error_message": "Please enter a valid option number...",
        "severity": "error"
      },
      {
        "field": "user_access",
        "rule": "Regular users cannot access admin-only options",
        "error_message": "No access - Admin Only option...",
        "severity": "error"
      },
      {
        "field": "authentication",
        "rule": "User must be authenticated (COMMAREA length > 0)",
        "error_message": "User not authenticated, redirecting to sign-on",
        "severity": "error"
      }
    ],
    "calculations": [
      {
        "name": "Input Length Calculation",
        "description": "Determines the actual length of user input by finding the rightmost non-space character",
        "formula": "Iterate from right to left until non-space character found or beginning reached",
        "input_fields": [
          "user_option_input"
        ],
        "output_field": "trimmed_input_length"
      },
      {
        "name": "Date Formatting",
        "description": "Formats current date as MM/DD/YY",
        "formula": "Extract month, day, and last 2 digits of year from current date",
        "input_fields": [
          "current_date"
        ],
        "output_field": "formatted_date"
      },
      {
        "name": "Time Formatting",
        "description": "Formats current time as HH:MM:SS",
        "formula": "Extract hours, minutes, and seconds from current time",
        "input_fields": [
          "current_time"
        ],
        "output_field": "formatted_time"
      }
    ],
    "workflows": [
      {
        "name": "Main Menu Flow",
        "description": "Primary workflow for displaying and processing the main menu",
        "steps": [
          {
            "step_number": 1,
            "action": "Initialize error flag and clear messages",
            "conditions": [],
            "next_step": "2"
          },
          {
            "step_number": 2,
            "action": "Check user authentication",
            "conditions": [
              "COMMAREA length equals 0"
            ],
            "next_step": "3_auth_fail or 4"
          },
          {
            "step_number": 3,
            "action": "Return to sign-on screen (authentication failed)",
            "conditions": [
              "User not authenticated"
            ],
            "next_step": "end"
          },
          {
            "step_number": 4,
            "action": "Load session data from COMMAREA",
            "conditions": [
              "User authenticated"
            ],
            "next_step": "5"
          },
          {
            "step_number": 5,
            "action": "Check if first entry or reentry",
            "conditions": [
              "Program reenter flag is false"
            ],
            "next_step": "6_first or 7_reentry"
          },
          {
            "step_number": 6,
            "action": "Initialize and display menu (first entry)",
            "conditions": [
              "First entry"
            ],
            "next_step": "end"
          },
          {
            "step_number": 7,
            "action": "Receive user input",
            "conditions": [
              "Reentry"
            ],
            "next_step": "8"
          },
          {
            "step_number": 8,
            "action": "Evaluate user action (Enter, PF3, or other)",
            "conditions": [],
            "next_step": "9_enter or 10_pf3 or 11_invalid"
          },
          {
            "step_number": 9,
            "action": "Process Enter key (validate and route)",
            "conditions": [
              "Enter key pressed"
            ],
            "next_step": "end"
          },
          {
            "step_number": 10,
            "action": "Process PF3 (exit to sign-on)",
            "conditions": [
              "PF3 key pressed"
            ],
            "next_step": "end"
          },
          {
            "step_number": 11,
            "action": "Handle invalid key",
            "conditions": [
              "Other key pressed"
            ],
            "next_step": "end"
          }
        ]
      },
      {
        "name": "Option Selection Processing",
        "description": "Workflow for processing user menu option selection",
        "steps": [
          {
            "step_number": 1,
            "action": "Trim and normalize user input",
            "conditions": [],
            "next_step": "2"
          },
          {
            "step_number": 2,
            "action": "Convert input to numeric",
            "conditions": [],
            "next_step": "3"
          },
          {
            "step_number": 3,
            "action": "Validate option number",
            "conditions": [
              "Option is numeric",
              "Option > 0",
              "Option <= menu option count"
            ],
            "next_step": "4_valid or error"
          },
          {
            "step_number": 4,
            "action": "Check user access permissions",
            "conditions": [
              "User type matches required type"
            ],
            "next_step": "5_authorized or error"
          },
          {
            "step_number": 5,
            "action": "Check if option is implemented",
            "conditions": [
              "Program name does not start with DUMMY"
            ],
            "next_step": "6_navigate or 7_coming_soon"
          },
          {
            "step_number": 6,
            "action": "Transfer to selected program",
            "conditions": [
              "Option is implemented"
            ],
            "next_step": "end"
          },
          {
            "step_number": 7,
            "action": "Display coming soon message",
            "conditions": [
              "Option not yet implemented"
            ],
            "next_step": "end"
          }
        ]
      }
    ],
    "integrations": [
      {
        "name": "User Security File",
        "type": "Data Store",
        "purpose": "Stores user authentication and authorization information",
        "data_exchanged": [
