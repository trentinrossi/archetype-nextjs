{
  "application_metadata": {
    "name": "CardDemo Admin Menu",
    "description": "Administrative menu system for the CardDemo application that provides centralized access to various administrative functions for authenticated admin users",
    "domain": "Financial Services - Credit Card Administration"
  },
  "backend": {
    "business_entities": [
      {
        "name": "AdminUser",
        "description": "Administrative user with access to admin functions",
        "attributes": [
          {
            "name": "user_id",
            "type": "string",
            "length": 8,
            "required": true,
            "description": "Unique identifier for the admin user"
          },
          {
            "name": "authentication_status",
            "type": "boolean",
            "length": 1,
            "required": true,
            "description": "Indicates if user has been authenticated as admin"
          }
        ],
        "relationships": [
          {
            "related_entity": "AdminMenuOption",
            "relationship_type": "has_access_to",
            "cardinality": "one-to-many"
          }
        ]
      },
      {
        "name": "AdminMenuOption",
        "description": "Individual administrative function available in the menu",
        "attributes": [
          {
            "name": "option_number",
            "type": "numeric",
            "length": 2,
            "required": true,
            "description": "Sequential number identifying the menu option"
          },
          {
            "name": "option_name",
            "type": "string",
            "length": 35,
            "required": true,
            "description": "Descriptive name of the administrative function"
          },
          {
            "name": "program_name",
            "type": "string",
            "length": 8,
            "required": true,
            "description": "Name of the program to execute for this option"
          },
          {
            "name": "is_active",
            "type": "boolean",
            "length": 1,
            "required": true,
            "description": "Indicates if option is active or coming soon (dummy)"
          }
        ],
        "relationships": [
          {
            "related_entity": "AdminProgram",
            "relationship_type": "executes",
            "cardinality": "one-to-one"
          }
        ]
      },
      {
        "name": "UserSession",
        "description": "Current user session information",
        "attributes": [
          {
            "name": "transaction_id",
            "type": "string",
            "length": 4,
            "required": true,
            "description": "Current transaction identifier (CA00)"
          },
          {
            "name": "program_name",
            "type": "string",
            "length": 8,
            "required": true,
            "description": "Current program name (COADM01C)"
          },
          {
            "name": "from_program",
            "type": "string",
            "length": 8,
            "required": false,
            "description": "Name of the program that transferred control"
          },
          {
            "name": "from_transaction",
            "type": "string",
            "length": 4,
            "required": false,
            "description": "Transaction ID of the calling program"
          },
          {
            "name": "program_context",
            "type": "numeric",
            "length": 9,
            "required": false,
            "description": "Context information for program state"
          },
          {
            "name": "reenter_flag",
            "type": "boolean",
            "length": 1,
            "required": true,
            "description": "Indicates if program is being reentered after user input"
          }
        ],
        "relationships": []
      }
    ],
    "business_rules": [
      {
        "id": "BR001",
        "name": "Admin Authentication Requirement",
        "description": "User must be authenticated as admin before accessing the admin menu",
        "category": "Security",
        "logic": "Program assumes user has been authenticated as admin user before reaching this menu. If no communication area is passed, user is returned to sign-on screen.",
        "conditions": [
          "Communication area length (EIBCALEN) equals 0"
        ],
        "actions": [
          "Set from program to COSGN00C",
          "Return to sign-on screen"
        ]
      },
      {
        "id": "BR002",
        "name": "Menu Option Count Limit",
        "description": "System can display up to 10 administrative menu options",
        "category": "Display",
        "logic": "Menu is limited to displaying a maximum of 10 options (OPTN001O to OPTN010O)",
        "conditions": [
          "Admin option count is defined in system configuration"
        ],
        "actions": [
          "Display options 1 through 10 only",
          "Ignore any options beyond position 10"
        ]
      },
      {
        "id": "BR003",
        "name": "Valid Option Selection",
        "description": "User must select a valid numeric option within the available range",
        "category": "Validation",
        "logic": "Option must be numeric, greater than zero, and not exceed the total count of available options",
        "conditions": [
          "Option is numeric",
          "Option is greater than zero",
          "Option is less than or equal to admin option count"
        ],
        "actions": [
          "If valid: navigate to selected program",
          "If invalid: display error message 'Please enter a valid option number...'"
        ]
      },
      {
        "id": "BR004",
        "name": "Dummy Option Handling",
        "description": "Options marked as dummy display coming soon message instead of executing",
        "category": "Navigation",
        "logic": "If selected option's program name starts with 'DUMMY', display coming soon message instead of transferring control",
        "conditions": [
          "Program name for selected option starts with 'DUMMY'"
        ],
        "actions": [
          "Display message 'This option is coming soon ...'",
          "Set message color to green",
          "Remain on admin menu screen"
        ]
      },
      {
        "id": "BR005",
        "name": "PF3 Exit to Sign-on",
        "description": "Pressing PF3 returns user to sign-on screen",
        "category": "Navigation",
        "logic": "When PF3 key is pressed, control is transferred back to the sign-on program",
        "conditions": [
          "User presses PF3 key"
        ],
        "actions": [
          "Set destination program to COSGN00C",
          "Transfer control to sign-on screen"
        ]
      },
      {
        "id": "BR006",
        "name": "Invalid Key Handling",
        "description": "Invalid function keys display error message",
        "category": "Error Handling",
        "logic": "Any key other than Enter or PF3 is considered invalid and triggers error message",
        "conditions": [
          "Key pressed is not Enter",
          "Key pressed is not PF3"
        ],
        "actions": [
          "Set error flag to 'Y'",
          "Display invalid key error message",
          "Redisplay menu screen"
        ]
      },
      {
        "id": "BR007",
        "name": "Initial Screen Display",
        "description": "On first entry, program displays menu screen without processing input",
        "category": "Display",
        "logic": "When program is entered for the first time (reenter flag is false), initialize screen and display menu",
        "conditions": [
          "Program reenter flag is false"
        ],
        "actions": [
          "Set reenter flag to true",
          "Clear screen output area",
          "Display menu screen",
          "Wait for user input"
        ]
      },
      {
        "id": "BR008",
        "name": "Program Context Initialization",
        "description": "When navigating to admin function, program context is reset",
        "category": "Navigation",
        "logic": "Before transferring to selected admin program, context is set to zeros and calling program information is stored",
        "conditions": [
          "Valid option selected",
          "Option is not dummy"
        ],
        "actions": [
          "Set from transaction ID to current transaction",
          "Set from program to current program name",
          "Set program context to zeros",
          "Transfer control to selected program"
        ]
      },
      {
        "id": "BR009",
        "name": "Dynamic Menu Building",
        "description": "Menu options are dynamically constructed from configuration data",
        "category": "Display",
        "logic": "Menu options are built at runtime from admin options data structure, combining option number and name",
        "conditions": [
          "Admin options data is available",
          "Option count is defined"
        ],
        "actions": [
          "Loop through available options",
          "Format each option as '[number]. [name]'",
          "Display in corresponding screen position"
        ]
      }
    ],
    "validations": [
      {
        "field": "option_selection",
        "rule": "Must be numeric",
        "error_message": "Please enter a valid option number...",
        "severity": "error"
      },
      {
        "field": "option_selection",
        "rule": "Must be greater than zero",
        "error_message": "Please enter a valid option number...",
        "severity": "error"
      },
      {
        "field": "option_selection",
        "rule": "Must not exceed admin option count",
        "error_message": "Please enter a valid option number...",
        "severity": "error"
      },
      {
        "field": "communication_area",
        "rule": "Must be present for authenticated access",
        "error_message": "Return to sign-on required",
        "severity": "error"
      },
      {
        "field": "function_key",
        "rule": "Must be Enter or PF3",
        "error_message": "Invalid key pressed",
        "severity": "error"
      }
    ],
    "calculations": [
      {
        "name": "Option Input Normalization",
        "description": "Converts user input string to numeric option value",
        "formula": "Remove trailing spaces, replace remaining spaces with '0', convert to numeric",
        "input_fields": [
          "option_input_string"
        ],
        "output_field": "option_numeric"
      },
      {
        "name": "Current Date Formatting",
        "description": "Formats system date for display",
        "formula": "Extract month, day, year (2 digits) and format as MM/DD/YY",
        "input_fields": [
          "system_current_date"
        ],
        "output_field": "display_date"
      },
      {
        "name": "Current Time Formatting",
        "description": "Formats system time for display",
        "formula": "Extract hours, minutes, seconds and format as HH:MM:SS",
        "input_fields": [
          "system_current_time"
        ],
        "output_field": "display_time"
      },
      {
        "name": "Menu Option Text Construction",
        "description": "Builds display text for each menu option",
        "formula": "Concatenate option number + '. ' + option name",
        "input_fields": [
          "option_number",
          "option_name"
        ],
        "output_field": "option_display_text"
      }
    ],
    "workflows": [
      {
        "name": "Admin Menu Main Flow",
        "description": "Primary workflow for admin menu interaction",
        "steps": [
          {
            "step_number": 1,
            "action": "Initialize program variables and clear error flags",
            "conditions": [
              "Program entry"
            ],
            "next_step": "2"
          },
          {
            "step_number": 2,
            "action": "Check for communication area presence",
            "conditions": [
              "Communication area length equals 0"
            ],
            "next_step": "3_return_to_signon"
          },
          {
            "step_number": 3,
            "action": "Load communication area data",
            "conditions": [
              "Communication area present"
            ],
            "next_step": "4"
          },
          {
            "step_number": 4,
            "action": "Check if first entry or reentry",
            "conditions": [
              "Reenter flag is false"
            ],
            "next_step": "5_initial_display"
          },
          {
            "step_number": 5,
            "action": "Process user input from screen",
            "conditions": [
              "Reenter flag is true"
            ],
            "next_step": "6"
          },
          {
            "step_number": 6,
            "action": "Evaluate function key pressed",
            "conditions": [
              "Enter key pressed",
              "PF3 pressed",
              "Other key pressed"
            ],
            "next_step": "7_process_enter or 8_exit or 9_error"
          },
          {
            "step_number": 7,
            "action": "Process Enter key - validate and navigate",
            "conditions": [
              "Enter key pressed"
            ],
            "next_step": "10_validate_option"
          },
          {
            "step_number": 8,
            "action": "Exit to sign-on screen",
            "conditions": [
              "PF3 pressed"
            ],
            "next_step": "end"
          },
          {
            "step_number": 9,
            "action": "Display invalid key error",
            "conditions": [
              "Invalid key pressed"
            ],
            "next_step": "5_initial_display"
          },
          {
            "step_number": 10,
            "action": "Validate option selection",
            "conditions": [
              "Option is valid",
              "Option is invalid"
            ],
            "next_step": "11_navigate or 12_error"
          },
          {
            "step_number": 11,
            "action": "Navigate to selected admin function",
            "conditions": [
              "Option is not dummy"
            ],
            "next_step": "end"
          },
          {
            "step_number": 12,
            "action": "Display coming soon message",
            "conditions": [
              "Option is dummy"
            ],
            "next_step": "5_initial_display"
          }
        ]
      },
      {
        "name": "Menu Screen Display",
        "description": "Workflow for preparing and displaying the admin menu screen",
        "steps": [
          {
            "step_number": 1,
            "action": "Populate header information with current date and time",
            "conditions": [],
            "next_step": "2"
          },
          {
            "step_number": 2,
            "action": "Build menu options from configuration",
            "conditions": [],
            "next_step": "3"
          },
          {
            "step_number": 3,
            "action": "Set error or informational message if present",
            "conditions": [],
            "next_step": "4"
          },
          {
            "step_number": 4,
            "action": "Display screen to user",
            "conditions": [],
            "next_step": "end"
          }
        ]
      },
      {
        "name": "Option Selection Processing",
        "description": "Workflow for processing user's menu option selection",
        "steps": [
          {
            "step_number": 1,
            "action": "Extract and normalize option input",
            "conditions": [],
            "next_step": "2"
          },
          {
            "step_number": 2,
            "action": "Convert to numeric value",
            "conditions": [],
            "next_step": "3"
          },
          {
            "step_number": 3,
            "action": "Validate option is numeric, non-zero, and within range",
            "conditions": [
              "Validation passes",
              "Validation fails"
            ],
            "next_step": "4_check_dummy or 5_error"
          },
          {
            "step_number": 4,
            "action": "Check if option is dummy or active",
            "conditions": [
              "Program name starts with DUMMY",
              "Program is active"
            ],
            "next_step": "6_coming_soon or 7_transfer"
          },
          {
            "step_number": 5,
            "action": "Display validation error message",
            "conditions": [],
            "next_step": "end"
          },
          {
            "step_number": 6,
            "action": "Display coming soon message",
            "conditions": [],
            "next_step": "end"
          },
          {
            "step_number": 7,
            "action": "Set context and transfer to selected program",
            "conditions": [],
            "next_step": "end"
          }
        ]
      }
    ],
    "integrations": [
      {
        "name": "Sign-on Screen Integration",
        "type": "Program Transfer",
        "purpose": "Return to authentication screen when session is invalid or user exits",
        "data_exchanged": [
          "from_program_name",
          "to_program_name"
        ],
        "trigger_conditions": [
          "No communication area present",
          "PF3 key pressed",
          "User requests exit"
        ]
      },
      {
        "name": "Admin Function Programs",
        "type": "Program Transfer",
        "purpose": "Navigate to specific administrative functions based on user selection",
        "data_exchanged": [
          "from_transaction_id",
          "from_program_name",
          "program_context",
          "communication_area"
        ],
        "trigger_conditions": [
          "Valid option selected",
          "Option is not dummy",
          "Enter key pressed"
        ]
      },
      {
        "name": "User Security File",
        "type": "Data Access",
        "purpose": "Validate user authentication and authorization",
        "data_exchanged": [
          "user_credentials",
          "admin_status"
        ],
        "trigger_conditions": [
          "User authentication required"
        ]
      }
    ],
    "security": {
      "roles": [
        "Admin User",
        "System Administrator"
      ],
      "permissions": [
        {
          "role": "Admin User",
          "resource": "Admin Menu",
          "actions": [
            "view_menu",
            "select_options",
            "navigate_to_admin_functions"
          ]
        }
      ],
      "authentication_requirements": [
        "User must be authenticated before accessing admin menu",
        "Valid communication area must be passed to program",
        "User must have admin privileges",
        "Session must be active and valid"
      ]
    },
    "error_handling": [
      {
        "error_code": "INVALID_KEY",
        "condition": "User presses function key other than Enter or PF3",
        "business_impact": "User cannot proceed with invalid action",
        "suggested_action": "Display error message and redisplay menu"
      },
      {
        "error_code": "INVALID_OPTION",
        "condition": "User enters non-numeric, zero, or out-of-range option",
        "business_impact": "User cannot navigate to desired function",
        "suggested_action": "Display validation error message and allow re-entry"
      },
      {
        "error_code": "NO_COMMAREA",
        "condition": "Communication area length is zero",
        "business_impact": "User session is invalid or not authenticated",
        "suggested_action": "Return user to sign-on screen"
      },
      {
        "error_code": "DUMMY_OPTION",
        "condition": "Selected option program name starts with DUMMY",
        "business_impact": "Requested function is not yet available",
        "suggested_action": "Display coming soon message in green"
      }
    ]
  },
